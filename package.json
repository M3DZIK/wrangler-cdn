{
  "name": "cdn",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "main": "dist/worker.js",
  "scripts": {
    "build": "webpack",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}'",
    "lint": "eslint --fix --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}'",
    "test": "jest --config jestconfig.json --verbose"
  },
  "author": "MedzikUser",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "devDependencies": {
    "@cloudflare/workers-types": "2.2.2",
    "@types/jest": "27.0.2",
    "@types/service-worker-mock": "2.0.1",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-typescript": "3.0.0",
    "jest": "27.3.1",
    "prettier": "2.4.1",
    "service-worker-mock": "2.0.5",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "typescript": "4.4.4",
    "webpack": "5.64.1",
    "webpack-cli": "4.9.1"
  }
}
